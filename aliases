# vim: ft=zsh

# Navigation
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."
alias .h="cd $HOME"

alias ll="ls -l"
alias la="ls -la"

# Common programs
alias g="git"
alias ag="sudo apt-get"
alias ac="apt-cache"
alias d="sudo docker"
alias t="tmux -2"
alias tn="tmux -2 new-session -s"
